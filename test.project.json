{
  "name": "plasma",
  "servePort": 34873,

  "tree": {
    "$className": "DataModel",
    "StarterPlayer": {
      "$className": "StarterPlayer",
      "StarterPlayerScripts": {
        "$className": "StarterPlayerScripts",
        "$path": "example"
      }
    },
    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",
      "$path": "DevPackages",
      "Plasma": {
        "$path": "src"
      },
      "Stories": {
        "$path": "stories"
      },
      "Tests": {
        "$path": "tests"
      }
    },
    "TestService": {
      "$properties": {
        "ExecuteWithStudioRun": true
      },
      "$className": "TestService",
      "run": {
        "$path": "tests.server.lua"
      }
    }
  }
}
